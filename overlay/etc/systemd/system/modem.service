[Unit]
Description=modem manage
After=set_time.service
[Service]
Environment="PYTHONPATH=/usr/local/share/droid4/python3_packages"
ExecStart=/usr/local/share/droid4/python3_packages/modem/main.py
ExecStartPre=/bin/sh -c "while [ ! -c /dev/motmdm1 ] ;do sleep 1; done"
[Install]
WantedBy=startup.target
